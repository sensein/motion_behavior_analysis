{
  "settings": [
    {
      "fps": 30,
      "resolution": [null, null],
      "format": "mp4"
    }
  ],
  "pose_estimation_configs": [
    {
      "pose_framework": "mediapipe",
      "pose_model": "BLAZEPOSE",
      "to_csv": false,
      "to_h5": false,
      "display": false,
      "save_images": true,
      "save_video": true,
      "model_complexity": 2
    }
  ],
  "calibration_configs": {
    "calibration_type": "calculate",
    "overwrite": false,
    "intrinsics": {
      "overwrite_intrinsics": false,
      "show_detection_intrinsics": true,
      "intrinsics_extension": "mp4",
      "extract_every_N_sec": 0.5,
      "intrinsics_corners_nb": [7,10],
      "intrinsics_square_size": 50
    }, 
    "extrinsics": {
      "calculate_extrinsics": true,
      "show_reprojection_error": true,
      "extrinsics_extension": "mp4",
      "extrinsics_corners_nb": [7,10],
      "extrinsics_square_size": 50
    }
  },
  "person_association": {
    "tracked_keypoint": "left_shoulder", 
    "reproj_error_threshold_association": 10, 
    "likelihood_threshold_association": 0.1
  }, 
  "triangulation": {
    "reproj_error_threshold_triangulation": 20, 
    "likelihood_threshold_triangulation": 0.3, 
    "min_cameras_for_triangulation": 2, 
    "interpolation": "linear", 
    "interp_if_gap_smaller_than": 10, 
    "show_interp_indices": true, 
    "handle_LR_swap": false, 
    "undistort_points": false
  }, 
  "filtering": {
    "display_figures": false, 
    "filters": {
      "butterworth": {
        "order": 4, 
        "cut_off_frequency": 6
      },
      "kalman": {
        "trust_ratio": 100,
        "smooth": true
      },
      "butterworth_on_speed": {
        "order": 4, 
        "cut_off_frequency": 10
      },
      "gaussian": {
        "sigma_kernel": 2
      },
      "LOESS": {
        "nb_values_used": 30
      },
      "median": {
        "kernel_size": 9
      }        
    }
  }
}